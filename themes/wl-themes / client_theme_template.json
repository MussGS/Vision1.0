{
  "xx": {
    "global": {
      "font": {
        "fontFamilies": {
          "display": {
            "$type": "fontFamilies",
            "$value": "IBM Plex Sans",
            "$description": "Font family for display fonts"
          },
          "content": {
            "$type": "fontFamilies",
            "$value": "IBM Plex Sans",
            "$description": "Font family for content fonts"
          }
        },
        "fontWeight": {
          "black": {
            "$type": "fontWeights",
            "$value": "Bold",
            "$description": "heaviest weight for fonts"
          },
          "bold": {
            "$type": "fontWeights",
            "$value": "Bold",
            "$description": "bold weight for fonts"
          },
          "semiBold": {
            "$type": "fontWeights",
            "$value": "SemiBold",
            "$description": "semibold weight for fonts"
          },
          "default": {
            "$type": "fontWeights",
            "$value": "Regular",
            "$description": "default weight for fonts"
          }
        }
      }
    },
    "alias": {
      "border": {
        "width": {
          "sm": {
            "$type": "borderWidth",
            "$value": "1px",
            "$description": "small thickness for borders"
          },
          "md": {
            "$type": "borderWidth",
            "$value": "3px",
            "$description": "medium border thickness"
          }
        },
        "focusOutline": {
          "$type": "border",
          "$value": {
            "color": "{xx.alias.color.border.focusOutline}",
            "width": "{xx.alias.border.width.md}",
            "style": "solid"
          },
          "$description": "default outline for focused elements"
        },
        "radius": {
          "sm": {
            "$type": "borderRadius",
            "$value": "0",
            "$description": "small border radius"
          },
          "md": {
            "$type": "borderRadius",
            "$value": "4px",
            "$description": "medium border radius"
          },
          "lg": {
            "$type": "borderRadius",
            "$value": "8px",
            "$description": "large border radius"
          }
        }
      },
      "color": {
        "primary": {
          "interaction": {
            "$type": "color",
            "$value": "#DC7258",
            "$description": "Color that indicates interactions"
          },
          "onInteraction": {
            "$type": "color",
            "$value": "#f5f5fa",
            "$description": "color for elements that are placed on interactive background colors"
          },
          "brand": {
            "onPrimary": {
              "value": "{xx.global.color.mint.100}",
              "type": "color",
              "description": "color for elements that are placed on primary brand color"
            },
            "$type": "color",
            "$value": "#3333C2",
            "$description": "background color for primary brand elements "
          },
          "onBrand": {
            "$type": "color",
            "$value": "#f5f5fa",
            "$description": "color for elements that are placed on brand background color"
          }
        },
        "purpose": {
          "infoHighEmphasis": {
            "$type": "color",
            "$value": "#2980b9",
